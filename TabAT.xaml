<UserControl x:Class="Traderdata.Client.TerminalWEB.TabAT"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="325" d:DesignWidth="602" xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk" xmlns:c1="clr-namespace:C1.Silverlight;assembly=C1.Silverlight">
    
    <Grid x:Name="LayoutRoot" Background="White">
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        
        <sdk:TabControl Margin="2,24,2,2" Name="tabControl" SelectionChanged="tabControl_SelectionChanged">
            <sdk:TabItem Header="+" Name="tabItem">
                <Grid />
            </sdk:TabItem>
        </sdk:TabControl>
        
        <c1:C1Menu HorizontalAlignment="Stretch" Name="c1Menu1" VerticalAlignment="Top">
            <c1:C1MenuItem Header="Menu1">
                <c1:C1MenuItem Header="SubMenu1"/>
            </c1:C1MenuItem>
        </c1:C1Menu>
    </Grid>
</UserControl>
